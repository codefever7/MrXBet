{
  "expo": {
    "name": "MrXBet",
    "slug": "MrXBet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.devservices.mrxbet"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/onesignal-notification-icon.png",
      "notification": {
        "smallIcon": "./assets/onesignal-notification-icon.png"
      },
      "package": "com.devservices.mrxbet",
      "versionCode": 2
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/onesignal-notification-icon.png",
      "androidMode": "default",
      "smallIcons": [
        "./assets/onesignal-notification-icon.png"
      ]
    },
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.devservices.mrxbet.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.devservices.mrxbet.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "9e7e1fe9-2ab6-46f1-9dbe-c0c935e6bea8"
      }
    }
  }
}
